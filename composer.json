{
    "name": "chenzel/custom-framework",
    "description": "Custom Framework with symfony 2 components",
    "homepage": "https://github.com/CHenzel/Custom-Framework",
    "license": "GPL V3",
    "authors": [
        {
            "name": "CHENZEL Custom Framework",
            "homepage": "http://chenzel.github.io/Custom-Framework"
        }
    ],
    "require": {
        "php": ">=5.4",
        "propel/propel": "2.0.*@dev",
        "symfony/routing": "3.0.*@dev",
        "symfony/templating": "3.0.*@dev",
        "symfony/http-foundation": "3.0.*@dev",
        "symfony/finder": "2.7.*@dev",
        "symfony/filesystem": "2.7.*@dev"
    },
    "autoload": {
        "classmap": ["src/model/"],
        "psr-4": { "app\\": "src/", "routing\\" : "routing/","lib\\" : "lib/" }
    }
}